{
  "version": 4,
  "include": [
    "base.json"
  ],
  "configurePresets": [
    {
      "name": "android.armv8",
      "inherits": "base",
      "toolchainFile": "$env{VS_WORKSPACE}/00_build_env/conan/_generated/host/android/armv8/conan_toolchain.cmake",
      "architecture": 
      {
        "value": "armv8",
        "strategy": "external"
      },
      "cacheVariables": 
      {
        "ARIEO_HOST_OS": "android",
        "TARGET_TRIPLE": "aarch64-linux-android35",
        "CMAKE_PREFIX_PATH": {
          "type": "PATH",
          "value": "$env{VS_WORKSPACE}/01_third_parties/conan/_generated/android/armv8;$env{VS_WORKSPACE}/01_third_parties/gradle/_generated/android/arm64-v8a/cmake"
        }
      }
    }
  ]
}
